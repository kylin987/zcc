<link rel="stylesheet" type="text/css" href="__TPL__/css/Checkable.css" />
<div id="index_con">
   <p style="text-align:center;">
   <img src="__TPL__/images/indl/W06-13.png" /></p>   
</div>
<form action="{:url('user/regain_sell_spree')}" method="post" enctype="multipart/form-data" name="formMsg">
  <section class="ect-bg-colorf user-service flow-consignee">
    <ul>
      <li class="input-text" style="text-align:center;">
	  	<strong>已驳回出售礼包<br></strong>({$spree_info.spree_sn})
      </li>
      <li>
        <div class="input-text"><b>售&nbsp;&nbsp;价：</b><span>
          {$spree_info.sell_price}
          </span></div>
      </li>
      <li>
        <div class="input-text"><b>留&nbsp;&nbsp;言：</b>
          {$spree_info.sell_beizhu}
        </div>
      </li>
	  <li>
        <div class="input-text"><b>驳回理由：</b>
          {$spree_info.admin_note}
        </div>
      </li>
    </ul>
  </section>
  <input type="hidden" name="spree_id" value="{$spree_info.id}" />
  <div class="two-btn ect-padding-tb ect-padding-lr ect-margin-tb text-center">
    <input type="submit" value="我要再次出售" class="btn btn-info ect-bg-colory" />
  </div>
</form>
<footer>
    <nav class="ect-nav2"><!-- #BeginLibraryItem "/library/page_menu2.lbi" --><!-- #EndLibraryItem --></nav>
</footer>
  <div style="padding-bottom:4.2em;"></div>
</div>
